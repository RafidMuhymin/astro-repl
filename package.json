    },
    "devDependencies": {
        "@types/node": "^16.7.1",
        "astring": "^1.7.5",
        "buffer": "^6.0.3",
        "esbuild": "^0.12.22",
        "esbuild-register": "^3.0.0",
        "estree-util-value-to-estree": "^1.2.0",
        "globby": "^12.0.1",
        "shorthash": "^0.0.2"
    },
    "dependencies": {
        "@astrojs/compiler": "^0.1.0-canary.34",
        "assert": "^2.0.0",
        "browser-builtins": "^3.3.1",
        "esbuild-wasm": "^0.12.22",
        "memfs": "^3.2.2",
        "monaco-editor": "^0.27.0",
        "path": "^0.12.7",
        "stream": "^0.0.2",
        "util": "^0.12.4"
    }
}
