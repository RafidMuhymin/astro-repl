{
    "name": "@astrojs/repl",
    "volta": {
        "node": "16.5.0"
    },
    "scripts": {
        "dev": "node -r esbuild-register scripts/build.ts --watch",
        "copy": "cp src/index.html dist/index.html && cp node_modules/esbuild-wasm/esbuild.wasm dist/esbuild.wasm && cp node_modules/@astrojs/compiler/astro.wasm dist/astro.wasm",
